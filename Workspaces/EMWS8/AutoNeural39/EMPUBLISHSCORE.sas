IF _DM_BAD EQ 0 THEN DO;
   H1x1_1  =    -0.59619526510279 * S_AcceptedCmpTotal
          +    -0.19567367757209 * S_Age  +    -1.87154193856682 * S_Frq
          +    -0.61220463973763 * S_Income  +     0.39435632091859 *
        S_Kidhome  +     0.27115687172661 * S_MntFishProducts
